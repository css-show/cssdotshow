{
  "name": "cssdotshow",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8008",
    "build": "next build && next export",
    "lint": "next lint",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "dependencies": {
    "@cloudbase/js-sdk": "^1.7.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "gray-matter": "^4.0.3",
    "next": "^12.0.10",
    "next-i18next": "^8.10.0",
    "next-pwa": "^5.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark": "^13.0.0",
    "remark-html": "^13.0.2",
    "rxjs": "^7.5.4",
    "sass": "^1.49.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@types/node": "^14.18.10",
    "@types/react": "^17.0.39",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.6",
    "semantic-release": "^17.4.7",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/css-show/cssdotshow.git"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "The CSS Show 变更如下："
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "./out",
              "label": "The CSS Show",
              "name": "css.show files"
            }
          ]
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "components/*",
            "lib/*",
            "pages/*",
            "posts/*",
            "public/*",
            "styles/*",
            "CHANGELOG.md",
            "package.json",
            "yarn.lock"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/css-show/cssdotshow/issues"
  },
  "homepage": "https://github.com/css-show/cssdotshow#readme",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "css"
  ],
  "author": "zhanghe",
  "license": "ISC"
}
